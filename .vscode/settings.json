{
    "files.exclude": {
        "**/__pycache__": true,
        ".venv/**": true,
        ".idea/**": true,
        ".serverless/**": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    },
    "code-runner.executorMap": {
        "python": " pipenv run python $fullFileName",
        "typescript": " npx tsx $fullFileName"
    },
    "code-runner.runInTerminal": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
}
